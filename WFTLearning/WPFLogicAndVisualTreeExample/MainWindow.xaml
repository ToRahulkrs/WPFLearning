<Window x:Class="WPFLogicAndVisualTreeExample.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFLogicAndVisualTreeExample"  
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel TextElement.FontSize="22" Orientation="Vertical" x:Name="Panel">
        <TextBlock Text="Some Text" Margin="15" Width="120" Height="40"  local:Element.SecurityId="a1" />
        <Button x:Name="CheckId" Content="Button" Click="chickId_OnClick" Margin="15" local:Element.SecurityId="a2" />
    </StackPanel>
</Window>
